{
  "openapi": "3.0.2",
  "info": {
    "title": "API Title",
    "version": "1.0"
  },
  "servers": [
    {
      "url": "http://localhost:3000/api"
    }
  ],
  "paths": {
    "/cathingory": {
      "get": {
        "operationId": "listCathingorys",
        "summary": "Listar todas las cathingorias disponibles",
        "responses": {
          "200": {
            "description": "Respuesta 200",
            "content": {
              "application/json": {
                "examples": {
                  "default": {
                    "value": [
                      {
                        "id": 1,
                        "name": "Deporte",
                        "description": "Deporte qué mas quieres",
                        "image": "https://i1.sndcdn.com/artworks-xPbDHcvYlR3JByOv-L7g2uw-t500x500.jpg"
                      },
                      {
                        "id": 2,
                        "name": "Películas",
                        "description": "Contenido multimedia de una duración de más de 60 mins",
                        "image": "https://m.media-amazon.com/images/I/81piFfIT01L._AC_UF894,1000_QL80_.jpg"
                      }
                    ]
                  }
                }
              }
            }
          }
        }
      }
    },
    "/cathingory/:name": {
      "get": {
        "summary": "Listar todas las cathingorías que coincidan con name",
        "parameters": [
          {
            "name": "name",
            "description": "El nombre de la persona",
            "allowEmptyValue": true,
            "in": "path"

          }
        ],
        "responses": {
          "200": {
            "description": "te devuelve un objeto json con todas las categorías que coincidan con el nombre",
            "content": {
              "application/json": {
                "examples": {
                  "default": {
                    "value": [
                      {
                        "id": 1,
                        "name": "Deporte",
                        "description": "Deporte qué mas quieres",
                        "image": "https://i1.sndcdn.com/artworks-xPbDHcvYlR3JByOv-L7g2uw-t500x500.jpg"
                      }
                    ]
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
